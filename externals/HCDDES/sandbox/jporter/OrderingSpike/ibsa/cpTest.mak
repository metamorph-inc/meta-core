OUTFILE=$(OUTDIR)/cpTest
CFG_INC=-I../include 
CFG_LIB=
CFG_OBJ=
COMMON_OBJ=$(OUTDIR)/cpTest.o $(OUTDIR)/DepGraph.o 
OBJ=$(COMMON_OBJ) $(CFG_OBJ)
ALL_OBJ=$(OUTDIR)/cpTest.o $(OUTDIR)/DepGraph.o 

COMPILE=g++ -c    -g -o "$(OUTDIR)/$(*F).o" $(CFG_INC) "$<"
